  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/bridbitbucket/bridbitbucket.bitbucket.io.git"
  },
  "homepage": "https://bitbucket.org/bridbitbucket/bridbitbucket.bitbucket.io",
  "files": [
    "dist/",
    "hydrate/",
    "loader/",
    "www/",
    "custom-elements-vscode.json",
    "custom-elements.json"
  ],
  "scripts": {
    "build": "npm run analyze && stencil build",
    "dev": "npm run build && concurrently \"npm run analyze -- --watch\" \"npm run build -- --watch\" \"npm run storybook\"",
    "www": "stencil build --dev --watch --serve",
    "analyze": "custom-elements-manifest analyze --globs \"src/**/*.tsx\" --stencil",
    "build-storybook": "storybook build",
    "storybook": "storybook dev -p 6007 --quiet",
    "ignore-sync": "ignore-sync .",
    "plop": "stencil-base-plop",
    "test": "stencil test --spec --passWithNoTests",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "npm run stylelint -- --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "qa": "npm run test && npm run lint && npm run format:check",
    "chromatic": "npx chromatic --project-token=chpt_401efb1fc155f30"
  },
  "dependencies": {
