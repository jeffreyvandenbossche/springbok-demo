    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.3.0",
    "chromatic": "^11.0.0",
    "concurrently": "^7.6.0",
    "ignore-sync": "^6.0.2",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "lit": "^2.7.2",
